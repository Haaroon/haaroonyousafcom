<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<staticContent>
			<remove fileExtension=".svg" />
			<mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
		</staticContent>     
        <remove statusCode="404" subStatusCode="-1" />
        <error statusCode="404" path="/404.html" responseMode="ExecuteURL" />   
      </httpErrors>
	</system.webServer>
</configuration>